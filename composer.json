{
    "name": "jakub-onderka/openid-connect-php",
    "description": "Bare-bones OpenID Connect client",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.0",
        "phpseclib/phpseclib" : ">=3.0.7",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "roave/security-advisories": "dev-master"
    },
    "archive" : {
        "exclude" : [
            ".*"
        ]
    },
    "autoload" : {
         "classmap": [ "src/"]
    }
}
